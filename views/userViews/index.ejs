  <script>
   

    const userIcon = document.getElementById('user-icon');
const dropdown = document.getElementById('user-dropdown');

// Show dropdown on hover or focus
userIcon.addEventListener('mouseenter', showDropdown);
userIcon.addEventListener('focus', showDropdown);

// Hide dropdown on mouse leave or blur
userIcon.addEventListener('mouseleave', hideDropdown);
dropdown.addEventListener('mouseleave', hideDropdown);
userIcon.addEventListener('blur', hideDropdown);

// Hide dropdown when clicking outside
document.addEventListener('click', function (event) {
    if (!userIcon.contains(event.target) && !dropdown.contains(event.target)) {
        hideDropdown();
    }
});

// Function to show dropdown
function showDropdown() {
    dropdown.style.display = 'block';
}

// Function to hide dropdown
function hideDropdown() {
    dropdown.style.display = 'none';
}





  </script>

